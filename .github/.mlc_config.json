{
  "ignorePatterns": [
    {
      "pattern": [
        "^https://deno.land/(std|api)@"
      ]
    },
    {
      "pattern": [
        "^https://github.com/denoland/deno/blob/"
      ]
    },
    {
      "pattern": [
        "^https://deno.land/images/schematic_v0.2.png"
      ]
    },
    {
      "pattern": [
        "^https://kinsta.com/signup/"
      ]
    },
    {
      "pattern": [
        "^http://(localhost|0.0.0.0)"
      ]
    },
    {
      "pattern": [
        "^/api"
      ]
    },
    {
      "pattern": [
        "^https://dash.cloudflare.com"
      ]
    },
    {
      "pattern": [
        "^https://web.pulsar-edit.dev/"
      ]
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://crates.io"],
      "headers": {
        "accept": "text/html"
      }
    },
    {
      "urls": ["https://deno.land"],
      "headers": {
        "accept": "text/html"
      }
    }
  ]
}
